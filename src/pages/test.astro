---
import { pinia } from "../store/pinia";
import Layout from '../layouts/Main.astro'

import { useTestStore } from '../store/teststore'
const testStore = useTestStore(pinia)
testStore.arr = [
	'testValue'
]
---
<Layout>
	<h1>Astro</h1>
	<ul>
		{
			testStore.arr.map(item => (
				<li>{item}</li>
			))
		}
	</ul>
	<a href="/">Index</a>
</Layout>
